<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.wuguanjingyao.ChangJianWenTiActivity">


    <ImageView
        android:id="@+id/fanhui"
        android:layout_marginTop="16dp"
        android:padding="2dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/fanhui_hei"
        android:layout_width="15dp"
        android:layout_height="20dp"/>


    <TextView
        app:layout_constraintTop_toTopOf="@+id/fanhui"
        app:layout_constraintBottom_toBottomOf="@+id/fanhui"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="18sp"
        android:textColor="#333333"
        android:text="常见问题"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <View
        android:id="@+id/ggg"
        app:layout_constraintTop_toBottomOf="@+id/fanhui"
        android:layout_marginTop="16dp"
        android:background="#E5E5E5"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <View
        android:id="@+id/ggg2"
        app:layout_constraintTop_toBottomOf="@+id/ggg"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#E5E5E5"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>

    <TextView
        android:textColor="#AAAAAA"
        android:textSize="13sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="18dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ggg"
        android:text="钱包使用帮助"
        android:id="@+id/ggrr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <LinearLayout
        android:id="@+id/lll"
        android:orientation="vertical"
        android:layout_marginTop="18dp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/ggrr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:text="充值/转出方法及规则"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            
            <ImageView
                android:background="@drawable/you"
                android:layout_width="13dp"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_height="13dp"/>

            <View
                android:background="#E5E5E5"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl2"
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:text="充值/转出失败类问题"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:background="@drawable/you"
                android:layout_width="13dp"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_height="13dp"/>

            <View
                android:background="#E5E5E5"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl3"
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:text="钱包账单类问题"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:background="@drawable/you"
                android:layout_width="13dp"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_height="13dp"/>

            <View
                android:background="#E5E5E5"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl4"
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:text="支付密码类问题"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:background="@drawable/you"
                android:layout_width="13dp"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_height="13dp"/>

            <View
                android:background="#E5E5E5"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </RelativeLayout>
    </LinearLayout>

    <TextView
        android:textColor="#AAAAAA"
        android:textSize="13sp"
        android:paddingStart="18dp"
        android:layout_marginTop="18dp"
        app:layout_constraintTop_toBottomOf="@+id/lll"
        android:text="虚拟礼物收益提现帮助"
        android:id="@+id/hhh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:id="@+id/lll333"
        android:orientation="vertical"
        android:layout_marginTop="18dp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/hhh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rl5"
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:text="收益扣税说明"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:background="@drawable/you"
                android:layout_width="13dp"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_height="13dp"/>

            <View
                android:background="#E5E5E5"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl6"
            android:layout_width="match_parent"
            android:layout_height="44dp">

            <TextView
                android:textColor="#333333"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:text="能得到多少收益?"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:background="@drawable/you"
                android:layout_width="13dp"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_height="13dp"/>

            <View
                android:background="#E5E5E5"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </RelativeLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>